#include <stdio.h>

int main() {
     int x1,y1,x2,y2;
     int midx,midy;
     printf("Please enter the number of x1:");
     scanf("%d",&x1);
     printf("Please enter the number of y1: ");
     scanf("%d",&y1);
     printf("Please enter the number of x2: ");
     scanf("%d",&x2);
     printf("Please enter the number of y2: ");
     scanf("%d",&y2);
     midx= (x1+x2)/2;
     midy=(y1+y2)/2;
     printf("The x midpoint coordinate is: %d ",midx);
     printf("The y midpoint coordinate is: %d ",midy);

     return 0;
}
